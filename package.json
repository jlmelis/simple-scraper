{
  "name": "simple-scraper",
  "version": "1.0.0",
  "description": "Simple web scrapping practice for use in another project. Also using Netlify functions to host.",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "npm-run-all --parallel build:**",
    "build:app": "rollup -c",
    "build:functions": "netlify-lambda build functions",
    "dev": "npm-run-all --parallel dev:app dev:functions dev:proxy",
    "dev:proxy": "node expressProxy.js",
    "dev:app": "rollup -c -w",
    "dev:functions": "netlify-lambda serve functions",
    "start": "sirv public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "http-proxy-middleware": "^1.0.3",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "sirv-cli": "^0.4.4"
  }
}
